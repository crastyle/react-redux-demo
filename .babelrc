{
  "presets": [
    ["env", { "modules": false }],
    "react",
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["import", { "libraryName": "antd-mobile", "style": "css" }],
    ["import", { "libraryName": "antd", "style": "css" }]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "react", "es2015", "stage-2" ],
      "plugins": [ "istanbul", ["transform-class-properties", { "spec": true }], "transform-runtime", "transform-decorators-legacy" ]
    }
  },
  "ignore": [
    "build",
    "node_modules",
    "static",
    "test"
  ]
}
